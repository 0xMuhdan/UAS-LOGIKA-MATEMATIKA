{
    "builds": [
        {
            "src": "web_app_enhanced.py",
            "use": "@vercel/python",
            "config": { "includeFiles": ["static/**"] }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "web_app_enhanced.py"
        }
    ]
}
